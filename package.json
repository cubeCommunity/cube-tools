{
  "name": "@cube-community/cube-tools",
  "version": "0.0.3",
  "description": "Useful JS/TS tools for Cube and Cube queries",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "check": "npm run check:ts && npm run check:lint && npm run check:prettier",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint --ext .ts .",
    "check:prettier": "prettier --check {src,test}/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cubeCommunity/cube-tools.git"
  },
  "keywords": [
    "cube",
    "cubejs",
    "cubejs meta",
    "cubejs tools",
    "cube tools"
  ],
  "author": "Viktor Loktionov",
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/cubeCommunity/cube-tools/issues"
  },
  "homepage": "https://github.com/cubeCommunity/cube-tools#readme",
  "devDependencies": {
    "@cubejs-client/core": "^0.32.0",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
